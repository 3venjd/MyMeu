@model MEU.web.Models.LineUpViewModel


@{
    ViewData["Title"] = "AddLineUp";
}

<h2>AddLineUp</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="AddLineUp">
            <input type="hidden" asp-for="Terminal_id" />

            <partial name="_LineUp" />
           
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>

        </form>
    </div>
</div>