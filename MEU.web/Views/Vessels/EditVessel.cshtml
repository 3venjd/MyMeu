@model MEU.web.Models.VesselViewModel
@{
    ViewData["Title"] = "EditVessel";
}

<h2>Edit Vessel</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="EditVessel" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />

            <partial name="_Vessel" />

            <div class="form-group">
                <input type="submit" value="Edit Vessel" class="btn btn-primary" />


            </div>

        </form>


    </div>

</div>
