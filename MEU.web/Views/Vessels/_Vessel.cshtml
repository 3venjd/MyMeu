@model MEU.web.Models.VesselViewModel

<div class="form-group">
    <label asp-for="VesselTypeId" class="control-label"></label>
    <select asp-for="VesselTypeId" asp-items="Model.VesselTypes" class="form-control"></select>
    <span asp-validation-for="VesselTypeId" class="text-danger"></span>

</div>

<div class="form-group">
    <label asp-for="Vessel_Name" class="control-label"></label>
    <input asp-for="Vessel_Name" class="form-control" />
    <span asp-validation-for="Vessel_Name" class="text-danger"></span>

</div>

<div class="form-group">
    <label asp-for="ImageFile" class="control-label"></label>
    <input asp-for="ImageFile" class="form-control" type="file" />
    <span asp-validation-for="ImageFile" class="text-danger"></span>

</div>