@model MEU.web.Models.VesselViewModel
@{
    ViewData["Title"] = "AddVessel";
}

<h2>Add Vessel</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="AddVessel" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="_Vessel" />

            <div class="form-group">
                <input type="submit" value="AddVessel" class="btn btn-primary" />
            </div>

        </form>


    </div>

</div>