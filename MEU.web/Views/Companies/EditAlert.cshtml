@model MEU.web.Models.AlertsViewModel
@{
    ViewData["Title"] = "EditAlert";
}

<h2>Edit</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="EditAlert">
            <input type="hidden" asp-for="Status_id" />
            <input type="hidden" asp-for="Id" />
           
            <partial name="_Alert" />

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>

        </form>
    </div>
</div>