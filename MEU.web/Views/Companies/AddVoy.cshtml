@model MEU.web.Models.VoysViewModel
@{
    ViewData["Title"] = "AddVoy";
}

<h2>Add Voy</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="AddVoy">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Company_id" />

            <partial name="_Voy" />
            
            <div class="form-group">
                <input type="submit" value="Add Voy" class="btn btn-default" />
            </div>

        </form>


    </div>

</div>